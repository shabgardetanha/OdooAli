    "clean": "rm -rf build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "frontend/src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,json,md}": [
      "prettier --write"
    ]
  },

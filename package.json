    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "*",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",

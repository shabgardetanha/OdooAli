    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "unicorn": "^0.0.1",
    "url-loader": "*",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1",
